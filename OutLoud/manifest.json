{
    "name": "OutLoud",
    "version": "3.0.0",
    "description": "An extension that converts text to speech!",
    "permissions": [
        "storage",
        "tabs"
    ],
    "manifest_version": 3,
    "action": {
        "default_icon": {
            "16": "icon.png",
            "24": "icon.png",
            "32": "icon.png"
        },
        "default_title": "OutLoud",
        "default_popup": "index.html"
    },
    "author": "Fatima Ali"
}